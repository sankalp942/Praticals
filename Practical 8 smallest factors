import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
	    int div=2;
	    Scanner input = new Scanner(System.in);
        System.out.println("Enter Integer Value: ");
        int number= input.nextInt();
        while (number > 1) {
            if (number % div == 0) {
                System.out.println(div+",");
                number/=div;
            }
            else {
                div++;
            }
        }
    }
}
